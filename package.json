{
  "devDependencies": {
    "cypress": "^14.3.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "cypress-xpath": "^2.0.1"
  },
  "scripts": {
    "test:qauto": "cp cypress.env.qauto.json cypress.env.json && npx cypress run",
    "test:qauto2": "cp cypress.env.qauto2.json cypress.env.json && npx cypress run",
    "generate-report": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/output.json && mochawesome-report-generator cypress/reports/mochawesome/output.json"
  }
}
